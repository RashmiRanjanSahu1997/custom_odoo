<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.sequence" id="seq_account_loan">
            <field name="name">Loan Number</field>
            <field name="code">loan.number</field>
            <field name="prefix">LOAN/</field>
            <field name="padding">4</field>
            <field name="suffix">/%(year)s</field>
        </record>

        <record id="seq_account_loan" model="ir.sequence">
            <field name="name">Account loan sequence</field>
            <field name="code">account.loan</field>
            <field name="prefix">ACL</field>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>