<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="plastic_wizard_form_view" model="ir.ui.view">
        <field name="name">plastic.wizard.form.view</field>
        <field name="model">plastic.wizard</field>
        <field name="arch" type="xml">
            <form>
                
                <sheet>
                    <group>
                        <button name='sync_now' type='object' string='Sync Now1'/>
                        <button name='sync' type='object' string='sync now2'/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="plastic_wizard_action" model="ir.actions.act_window">
        <field name="name">Plastic Wizard</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">plastic.wizard</field>
        <field name="view_mode">form</field>
    </record>
</odoo>