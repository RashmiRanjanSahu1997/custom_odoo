<odoo>
    <record id="res_config_settings_form_view1" model="ir.ui.view">
        <field name="name">res.config.settings.form.view1</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="15"/>
        <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='group_warning_stock']" position="after">
                <div class="text-muted content-group mt16">
                    <group>
                        <field name="stock_limit" 
                                class="text-center oe_inline"/>
                    </group>
                    <group>
                    </group>
                </div>
            </xpath>
        </field>
    </record>
</odoo>