<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="project_inherit_view_kanban" model="ir.ui.view">
        <field name="name">project.inherit.view.kanban</field>
        <field name="model">project.project</field>
        <field name="inherit_id" ref="project.view_project_kanban"/>
        <field name="priority">50</field>
        <field name="arch" type="xml">
            <xpath expr="//div[hasclass('o_project_kanban_boxes')]" position="inside">
                <field name="total_timesheet"/>
                <a name="timesheet_view" type="object"> Time Sheet</a>
            </xpath>
        </field>
    </record>
</odoo>